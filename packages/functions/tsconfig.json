{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "es2020",
    "target": "es2017",
    "noImplicitReturns": true,
    // "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": [
      "vitest/globals",
      "vitest/importMeta"
    ]
  },
  "compileOnSave": true,
  "include": [
    "src"
  ]
}
