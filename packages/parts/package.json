{
  "name": "@living-dictionaries/parts",
  "version": "0.0.1",
  "description": "Language documentation components and helpers for use in livingdictionaries.app",
  "type": "module",
  "svelte": "src/lib/index.ts",
  "main": "src/lib/index.ts",
  "scripts": {
    "postinstall": "svelte-kit sync",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-kit package && node update-packaged-index-path.js",
    "check": "svelte-check --tsconfig ./tsconfig.json --threshold warning --diagnostic-sources js,svelte",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --threshold warning --diagnostic-sources js,svelte --watch",
    "release": "npm run package && cd package && npm publish",
    "test": "vitest -u"
  },
  "dependencies": {
    "ckeditor5-build-classic-with-alignment-underline-smallcaps": "^1.0.0"
  },
  "devDependencies": {
    "@living-dictionaries/types": "^1.0.0",
    "@sveltejs/adapter-auto": "1.0.0-next.80",
    "@sveltejs/kit": "1.0.0-next.505",
    "@turf/center": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@types/mapbox-gl": "^2.7.5",
    "@types/mapbox__mapbox-gl-geocoder": "^4.7.3",
    "@types/recordrtc": "^5.6.8",
    "@types/youtube": "^0.0.47",
    "kitbook": "^0.0.23",
    "mdsvex": "^0.10.6",
    "recordrtc": "^5.6.2",
    "replace-in-file": "^6.3.5",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.1",
    "svelte-deep-wind-preprocess": "^0.0.8",
    "svelte-pieces": "1.0.43",
    "svelte-preprocess": "^4.10.7",
    "svelte-store-router": "^2.0.10",
    "svelte-windicss-preprocess": "~4.1.0",
    "svelte2tsx": "^0.5.19",
    "sveltefirets": "^0.0.31",
    "tslib": "^2.4.0",
    "typescript": "~4.8.4",
    "vite": "^3.1.4",
    "vitest": "^0.23.4",
    "windicss": "^3.5.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/livingtongues/living-dictionaries.git"
  },
  "author": "Living Tongues Institute for Endangered Languages",
  "bugs": {
    "url": "https://github.com/livingtongues/living-dictionaries/issues"
  },
  "homepage": "https://github.com/livingtongues/living-dictionaries#readme"
}