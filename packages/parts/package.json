    "@resvg/resvg-js": "^2.2.0",
    "@sveltejs/adapter-auto": "1.0.0",
    "@sveltejs/kit": "1.0.1",
    "@turf/center": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@types/d3": "^7.4.0",
    "@types/mapbox-gl": "^2.7.6",
    "@types/mapbox__mapbox-gl-geocoder": "^4.7.3",
    "@types/recordrtc": "^5.6.10",
    "@types/youtube": "^0.0.47",
    "@unocss/preset-icons": "^0.48.0",
    "d3-geo": "^3.0.1",
    "kitbook": "^0.0.24",
    "lz-string": "^1.4.4",
    "mdsvex": "^0.10.6",
    "replace-in-file": "^6.3.5",
    "satori": "^0.0.44",
    "satori-html": "^0.3.0",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.1",
    "svelte-pieces": "1.0.56",
    "svelte-preprocess": "^5.0.0",
    "svelte-store-router": "^2.0.10",
    "svelte2tsx": "^0.6.0",
    "sveltefirets": "^0.0.32",
    "temp-s-p-u": "^0.0.7",
    "topojson-client": "^3.1.0",
    "tslib": "^2.4.1",
