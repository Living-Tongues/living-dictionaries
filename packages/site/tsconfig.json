{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es2020",
      "WebWorker",
      "DOM"
    ],
    "moduleResolution": "node",
    "module": "es2020",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "es2020",
    "types": [
      "vitest/globals",
      "vitest/importMeta"
    ]
  },
}